@model SportsStoreFrickingFinal.Models.ProducstListViewModel

@{
    ViewBag.Title = "List";
}

@foreach (var p in Model.Products)
{
    Html.RenderPartial("ProductSummary", p);
}
<div class="pager">
    @Html.PageLinks(Model.PagingInfoObject, x => Url.Action("List", new { page = x, category = Model.CurrentCategory }))
</div>

